.resolutionLoad-leave
    animation: slideOut 0.4s cubic-bezier(0.165, 0.84, 0.44, 1)
    opacity: 1.0
 
@keyframes slideIn
    0%
        opacity 0
        transform translate3d(-50px, 0, 0)
    100%
        opacity 1
        transform translate3d(0, 0, 0)
 
@keyframes slideOut
    0%
        opacity 1
        transform translate3d(0, 0, 0)
    100%
        opacity 0
        transform translate3d(50px, 0, 0)
 
@keyframes slideUp
    0%
        opacity 0
        transform translate3d(0, 60px, 0)
    100%
        opacity 1
        transform translate3d(0, 0, 0)
       
